            "post_content": "<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"blockGap\":\"0px\",\"padding\":{\"top\":\"0px\",\"bottom\":\"0px\"}}}} -->\n<div class=\"wp-block-group alignfull\" style=\"padding-top:0;padding-bottom:0;\"><!-- wp:media-text {\"align\":\"full\",\"mediaPosition\":\"right\",\"mediaId\":26,\"mediaLink\":\"https:\/\/marldemo.wordpress.com\/toa-heftiba-yn33ygmulwe-unsplash-2\/\",\"mediaType\":\"image\",\"imageFill\":false} -->\n<div class=\"wp-block-media-text alignfull has-media-on-the-right is-stacked-on-mobile\"><figure class=\"wp-block-media-text__media\"><img src=\"https:\/\/marldemo.files.wordpress.com\/2022\/02\/toa-heftiba-yn33ygmulwe-unsplash-edited.jpg?w=768\" alt=\"\" class=\"wp-image-26 size-full\" \/><\/figure><div class=\"wp-block-media-text__content\"><!-- wp:heading {\"style\":{\"typography\":{\"fontSize\":\"56px\"}}} -->\n<h2 id=\"contemporary-ceramics-handmade-in-australia\" style=\"font-size:56px;\">Contemporary ceramics, <em>handmade<\/em> in Australia.<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:buttons {\"style\":{\"spacing\":{\"margin\":{\"top\":\"20px\",\"bottom\":\"20px\"}}}} -->\n<div class=\"wp-block-buttons\" style=\"margin-top:20px;margin-bottom:20px;\"><!-- wp:button {\"style\":{\"border\":{\"radius\":\"100px\"},\"spacing\":{\"padding\":{\"top\":\"6px\",\"bottom\":\"6px\",\"left\":\"36px\",\"right\":\"36px\"}}},\"fontSize\":\"small\"} -->\n<div class=\"wp-block-button has-custom-font-size has-small-font-size\"><a class=\"wp-block-button__link\" href=\"#osaka-dinner-set\" style=\"border-radius:100px;padding-top:6px;padding-right:36px;padding-bottom:6px;padding-left:36px;\">SHOP NOW<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons --><\/div><\/div>\n<!-- \/wp:media-text -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"8vh\",\"bottom\":\"8vh\"}}},\"backgroundColor\":\"foreground\",\"textColor\":\"background\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-background-color has-foreground-background-color has-text-color has-background\" style=\"padding-top:8vh;padding-bottom:8vh;\"><!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"lineHeight\":\"1.3\"}},\"fontSize\":\"x-large\"} -->\n<h2 class=\"has-text-align-center has-x-large-font-size\" id=\"marl-is-a-small-pottery-studio-and-gallery-based-in-the-beautiful-byron-bay\" style=\"line-height:1.3;\">Marl is a small pottery studio and gallery, based in the beautiful Byron Bay.<\/h2>\n<!-- \/wp:heading --><\/div>\n<!-- \/wp:group --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"4vh\",\"padding\":{\"top\":\"8vh\",\"bottom\":\"8vh\"}}}} -->\n<div class=\"wp-block-group alignwide\" style=\"padding-top:8vh;padding-bottom:8vh;\"><!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"40px\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%;\"><!-- wp:image {\"id\":15,\"style\":{\"border\":{\"radius\":\"10px\"}},\"className\":\"is-style-default\"} -->\n<figure class=\"wp-block-image is-style-default\" style=\"border-radius:10px;\"><img src=\"https:\/\/marldemo.files.wordpress.com\/2022\/02\/toa-heftiba-shgxu7dwux4-unsplash.jpg?w=785\" alt=\"\" class=\"wp-image-15\" \/><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%;\"><!-- wp:paragraph {\"placeholder\":\"Content\u2026\",\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\">THE GOODS<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"typography\":{\"lineHeight\":\"1.3\"}},\"fontSize\":\"x-large\"} -->\n<h2 class=\"has-x-large-font-size\" id=\"all-items-are-100-hand-made-using-the-potter-s-wheel-or-traditional-techniques-created-with-love-and-care-in-australia\" style=\"line-height:1.3;\">Created with love and care in Australia.<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"lineHeight\":\"1.65\"}}} -->\n<p style=\"line-height:1.65;\">All items are 100% hand-made, using the potter\u2019s wheel or traditional techniques.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:list {\"style\":{\"typography\":{\"lineHeight\":\"2\"}}} -->\n<ul style=\"line-height:2;\"><li>Timeless style.<\/li><li>Earthy, organic feel.<\/li><li>Enduring quality.<\/li><li>Unique, one-of-a-kind pieces.<\/li><\/ul>\n<!-- \/wp:list --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:columns {\"align\":\"wide\",\"style\":{\"spacing\":{\"blockGap\":\"40px\"}}} -->\n<div class=\"wp-block-columns alignwide\"><!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%;\"><!-- wp:paragraph {\"placeholder\":\"Content\u2026\",\"fontSize\":\"small\"} -->\n<p class=\"has-small-font-size\">ABOUT US<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:heading {\"style\":{\"typography\":{\"lineHeight\":\"1.3\"}},\"fontSize\":\"x-large\"} -->\n<h2 class=\"has-x-large-font-size\" id=\"all-items-are-100-hand-made-using-the-potter-s-wheel-or-traditional-techniques-created-with-love-and-care-in-australia\" style=\"line-height:1.3;\">Marl is an independent studio and artisanal gallery.<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"style\":{\"typography\":{\"fontSize\":\"18px\"}}} -->\n<p style=\"font-size:18px;\">We specialize in limited collections of handmade tableware. We collaborate with restaurants and cafes to create unique items that complement the menu perfectly. Please get in touch if you would like to know more about our process and pricing.<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:buttons {\"style\":{\"spacing\":{\"margin\":{\"top\":\"20px\",\"bottom\":\"20px\"}}}} -->\n<div class=\"wp-block-buttons\" style=\"margin-top:20px;margin-bottom:20px;\"><!-- wp:button {\"style\":{\"border\":{\"radius\":\"100px\"},\"spacing\":{\"padding\":{\"top\":\"6px\",\"bottom\":\"6px\",\"left\":\"36px\",\"right\":\"36px\"}}},\"className\":\"is-style-outline\",\"fontSize\":\"small\"} -->\n<div class=\"wp-block-button has-custom-font-size is-style-outline has-small-font-size\"><a class=\"wp-block-button__link\" style=\"border-radius:100px;padding-top:6px;padding-right:36px;padding-bottom:6px;padding-left:36px;\">LEARN MORE<\/a><\/div>\n<!-- \/wp:button --><\/div>\n<!-- \/wp:buttons --><\/div>\n<!-- \/wp:column -->\n\n<!-- wp:column {\"width\":\"50%\"} -->\n<div class=\"wp-block-column\" style=\"flex-basis:50%;\"><!-- wp:image {\"id\":18,\"sizeSlug\":\"large\",\"linkDestination\":\"none\",\"style\":{\"border\":{\"radius\":\"10px\"}},\"className\":\"is-style-default\"} -->\n<figure class=\"wp-block-image size-large is-style-default\" style=\"border-radius:10px;\"><img src=\"https:\/\/marldemo.files.wordpress.com\/2022\/02\/toa-heftiba-nru6o8kg8uw-unsplash.jpg?w=819\" alt=\"\" class=\"wp-image-18\" \/><\/figure>\n<!-- \/wp:image --><\/div>\n<!-- \/wp:column --><\/div>\n<!-- \/wp:columns -->\n\n<!-- wp:image {\"align\":\"wide\",\"id\":21,\"sizeSlug\":\"large\",\"linkDestination\":\"none\",\"style\":{\"border\":{\"radius\":\"10px\"}}} -->\n<figure class=\"wp-block-image alignwide size-large\" style=\"border-radius:10px;\"><img src=\"https:\/\/marldemo.files.wordpress.com\/2022\/02\/toa-heftiba-srvmnmq_t-q-unsplash.jpg?w=1024\" alt=\"\" class=\"wp-image-21\" \/><\/figure>\n<!-- \/wp:image -->\n\n<!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"fontSize\":\"54px\"}}} -->\n<h2 class=\"has-text-align-center\" id=\"osaka-dinner-set\" style=\"font-size:54px;\">Osaka Dinner Set<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\"} -->\n<p class=\"has-text-align-center\"><s>$200.00<\/s> $150.00<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:jetpack\/recurring-payments -->\n<div class=\"wp-block-jetpack-recurring-payments\"><!-- wp:jetpack\/button {\"element\":\"a\",\"uniqueId\":\"recurring-payments-id\",\"text\":\"BUY NOW\",\"borderRadius\":50,\"width\":\"50%\",\"align\":\"center\"} \/--><\/div>\n<!-- \/wp:jetpack\/recurring-payments --><\/div>\n<!-- \/wp:group -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"8vh\",\"bottom\":\"8vh\"}}},\"backgroundColor\":\"tertiary\",\"layout\":{\"inherit\":true}} -->\n<div class=\"wp-block-group alignfull has-tertiary-background-color has-background\" style=\"padding-top:8vh;padding-bottom:8vh;\"><!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"small\"} -->\n<p class=\"has-text-align-center has-small-font-size\">WHAT PEOPLE ARE SAYING<\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:heading {\"textAlign\":\"center\",\"style\":{\"typography\":{\"lineHeight\":\"1.3\"}}} -->\n<h2 class=\"has-text-align-center\" id=\"these-plates-are-a-work-of-art-and-the-quality-is-outstanding-i-bought-one-set-as-a-gift-and-anoter-to-treat-myself-and-i-couldn-t-be-happier\" style=\"line-height:1.3;\">These plates are a work of art, and the quality is outstanding. I bought one set as a gift and anoter to treat myself, and I couldn't be happier!<\/h2>\n<!-- \/wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"small\"} -->\n<p class=\"has-text-align-center has-small-font-size\">- Terry B.<\/p>\n<!-- \/wp:paragraph --><\/div>\n<!-- \/wp:group -->",
